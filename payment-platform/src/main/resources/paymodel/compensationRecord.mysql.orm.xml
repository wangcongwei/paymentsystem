<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">
	<package>com.newtouch.payment.model</package>
	<access>PROPERTY</access>
	<!-- CompensationRecord -->
	<entity class="CompensationRecord">
		<table name="t_compensation_record" />
		<attributes>
			<id name="id">
				<column name="ID" />
				<generated-value strategy="IDENTITY" />
			</id>
			<basic name="transNo">
				<column name="trans_no" />
			</basic>
			<basic name="transStatus">
				<column name="trans_Status" />
			</basic>
			<basic name="nextTime">
				<column name="NEXT_TIME" />
			</basic>
			<basic name="compensationTimes">
				<column name="COMPENSATION_TIMES" />
			</basic>
			<basic name="creatDate">
				<column name="creat_date" />
			</basic>
			<basic name="updateDate">
				<column name="update_date" />
			</basic>
		</attributes>
	</entity>
</entity-mappings>
